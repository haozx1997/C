#include<bits/stdc++.h>

using namespace std;
char s[1001000];
char ss[1001000];

int main()
{
	int  t1= time();
	
	int c = 1000000
	for(int i =0;i<c;i++)
	{
		s[i] = '1';
	}
	int  t2= time();
	
	s[c] = 0;
	
	strcpy(ss,s);
	int  t3= time();
	printf("%lld\n %lld\n",t2-t1,t3-t2);
	
	



 return 0;
}


